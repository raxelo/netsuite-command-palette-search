{
  "name": "ns-command-palette-search",
  "displayName": "NetSuite Command Palette Search",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "description": "Allows you to press CMD + K and open an enhanced search on NetSuite, faster and packed with more features!",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev-firefox": "npm run clear && cross-env NODE_ENV=development EXTENSION=firefox run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.mts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.mts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "lint": "eslint --cache .",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/query-persist-client-core": "^5.45.0",
    "@tanstack/vue-query": "^5.45.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.396.0",
    "postcss-prefixer": "^3.0.0",
    "radix-vue": "^1.8.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue-command-palette": "^0.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.1",
    "@ffflorian/jszip-cli": "^3.6.2",
    "@iconify/json": "^2.2.191",
    "@playwright/test": "^1.42.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.26",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@unocss/reset": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.0.0",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-demi": "^0.14.7",
    "web-ext": "^7.11.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
